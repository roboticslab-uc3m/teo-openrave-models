<Robot name="rightDextra">

    <KinBody file="rightDextra.kinbody.xml" />

    <Manipulator name="thumb">
        <effector>RightThumbDistal</effector>   <!-- last link where end effector is attached-->
        <base>FrontalRightWrist</base>           <!-- base link-->
    </Manipulator>

    <Manipulator name="index">
        <effector>RightIndexFingerDistal</effector>   <!-- last link where end effector is attached-->
        <base>FrontalRightWrist</base>           <!-- base link-->
    </Manipulator>


    <Manipulator name="middle">
        <effector>RightMiddleFingerDistal</effector>   <!-- last link where end effector is attached-->
        <base>FrontalRightWrist</base>           <!-- base link-->
    </Manipulator>


    <Manipulator name="ring">
        <effector>RightRingFingerDistal</effector>   <!-- last link where end effector is attached-->
        <base>FrontalRightWrist</base>           <!-- base link-->
    </Manipulator>


    <Manipulator name="pinky">
        <effector>RightPinkyFingerDistal</effector>   <!-- last link where end effector is attached-->
        <base>FrontalRightWrist</base>           <!-- base link-->
    </Manipulator>

</Robot>
