<Robot name="teoSim">

    <KinBody name="base">
        <Body name="cintura" type="dynamic">
        <Geom type="trimesh">
            <Translation>0 0 0</Translation>
            <rotationaxis>0 0 1 0</rotationaxis>
            <Render>models/cintura_links.wrl</Render>
            <collision>models/cintura_links.wrl</collision>
        </Geom>
        <Mass type="custom">
            <density>1</density>
        </Mass>
        </Body>
    </KinBody>

    <!-- trunk -->
    <KinBody name="trunk" file="teo_trunk.kinbody.xml" />

    <!-- head -->
    <KinBody name="head" file="teo_head.kinbody.xml" />

    <!-- rightArm -->
    <KinBody name="rightArm" file="teo_rightArm.kinbody.xml" />

    <!-- rightHand -->
    <KinBody name="rightHand" file="teo_hand.kinbody.xml" />

    <!-- leftArm -->
    <KinBody name="leftArm" file="teo_leftArm.kinbody.xml" />

    <!-- rightLeg -->
    <KinBody name="rightLeg" file="teo_rightLeg.kinbody.xml" />

    <!-- leftLeg -->
    <KinBody name="leftLeg" file="teo_leftLeg.kinbody.xml" />

    <!-- Manipulators -->
  <Manipulator name="trunk">
    <effector>r31</effector>   <!-- last link where end effector is attached-->
    <base>cintura</base>           <!-- base link-->
    <!-- the id of the inverse kinematics solver, it is not necessary to-->
    <!-- specify an ik solver for every robot. But if there is one,-->
    <!-- use iksolver field to identity it.-->
    <!-- <iksolver>MyTriDOFSolver</iksolver> -->

    <Translation>0 0 0</Translation>
  </Manipulator>

  <Manipulator name="head">
    <effector>r01</effector>   <!-- last link where end effector is attached-->
    <base>r31</base>           <!-- base link-->
    <!-- the id of the inverse kinematics solver, it is not necessary to-->
    <!-- specify an ik solver for every robot. But if there is one,-->
    <!-- use iksolver field to identity it.-->
    <!-- <iksolver>MyTriDOFSolver</iksolver> -->

    <Translation>0 0 0</Translation>-->
  </Manipulator>

  <Manipulator name="rightArm">
    <effector>r14</effector>   <!-- last link where end effector is attached-->
    <base>r31</base>           <!-- base link-->
    <!-- the id of the inverse kinematics solver, it is not necessary to-->
    <!-- specify an ik solver for every robot. But if there is one,-->
    <!-- use iksolver field to identity it.-->
    <iksolver>WAM7ikfast 0.05</iksolver>
    <!-- <iksolver>MyTriDOFSolver</iksolver> -->
    <joints>RightThumbProximalJoint RightThumbDistalJoint RightExternalFingerProximalJoint RightExternalFingerDistalJoint RightInternalFingerProximalJoint RightInternalFingerDistalJoint</joints>
  <direction>0 0 -1</direction>
  <Translation>0 0 -0.2</Translation>
  <closingdirection>-30 -50 -30 -50 -30 -50</closingdirection>
  </Manipulator>

  <!-- KinBody: teo_hand -->	
 <kinbody>
   <body name="RightFrontalWrist">
     <offsetfrom>r14</offsetfrom>
   </body>
 
 <Joint circular="false" name="RightFrontalWristJoint" type="hinge"> 
	<Body>r14</Body>
	<Body>RightFrontalWrist</Body>
	<offsetfrom>RightFrontalWrist</offsetfrom>
	<weight>1.0</weight>
	<limitsdeg>-10 100</limitsdeg>
	<axis>0 -1 0</axis> 
	<maxvel>3</maxvel>
	<resolution>1</resolution>
  </Joint>
</kinbody>
