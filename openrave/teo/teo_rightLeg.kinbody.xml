
<!-- right leg  -->
<KinBody name="rightLeg" makejoinedlinksadjacent="1">

        <Body name="AxialRightHip" type="dynamic">
        <offsetfrom>RootWaist</offsetfrom>
        <Translation>0 -0.146 0</Translation>
        <RotationAxis>0 0 1 0</RotationAxis>
        <Geom type="trimesh">
        <Render>models/4.0^pierna_der_links.wrl</Render>
        <collision>models/4.0^pierna_der_links.wrl</collision>
        </Geom>
        <Mass type="custom">
          <density>1</density>
        </Mass>
        </Body>

        <Joint circular="false" name="AxialRightHip" type="hinge">
        <Body>RootWaist</Body>
        <Body>AxialRightHip</Body>
        <offsetfrom>AxialRightHip</offsetfrom>
        <weight>1.0</weight>
        <limitsdeg>-32.8 27.9</limitsdeg>
        <axis>0 0 1</axis>
        <maxveldeg>30</maxveldeg>
        <resolution>1</resolution>
        </Joint>

        <Body name="SaggitalRightHip" type="dynamic">
        <offsetfrom>AxialRightHip</offsetfrom>
        <Translation>0 0 -0.092</Translation>
        <RotationAxis>0 0 1 0</RotationAxis>
        <Geom type="trimesh">
        <Render>models/4.1^pierna_der_links.wrl</Render>
        <collision>models/4.1^pierna_der_links.wrl</collision>
        </Geom>
        <Mass type="custom">
          <density>1</density>
        </Mass>
        </Body>

        <Joint circular="false" name="SaggitalRightHip" type="hinge">
        <Body>AxialRightHip</Body>
        <Body>SaggitalRightHip</Body>
        <offsetfrom>SaggitalRightHip</offsetfrom>
        <weight>1.0</weight>
        <limitsdeg>-14.2 12.5</limitsdeg>
        <axis>1 0 0</axis>
        <maxveldeg>30</maxveldeg>
        <resolution>1</resolution>
        </Joint>


        <Body name="FrontalRightHip" type="dynamic">
        <Translation>0 0 0</Translation>
        <RotationAxis>0 0 1 0</RotationAxis>
        <offsetfrom>SaggitalRightHip</offsetfrom>
        <Geom type="trimesh">
        <Render>models/4.2^pierna_der_links.wrl</Render>
        <collision>models/4.2^pierna_der_links.wrl</collision>
        </Geom>
        <Mass type="custom">
          <density>1</density>
        </Mass>
        </Body>

        <Joint circular="false" name="FrontalRightHip" type="hinge">
        <Body>SaggitalRightHip</Body>
        <Body>FrontalRightHip</Body>
        <offsetfrom>FrontalRightHip</offsetfrom>
        <weight>1.0</weight>
        <limitsdeg>-31.6 42.3</limitsdeg>
        <axis>0 1 0</axis>
        <maxveldeg>30</maxveldeg>
        <resolution>1</resolution>
        </Joint>

        <Body name="FrontalRightKnee" type="dynamic">
        <offsetfrom>FrontalRightHip</offsetfrom>
        <Translation>0 0 -0.33</Translation>
        <Geom type="trimesh">
        <Render>models/4.3^pierna_der_links.wrl</Render>
        <collision>models/4.3^pierna_der_links.wrl</collision>
        </Geom>
        <Mass type="custom">
          <density>1</density>
        </Mass>
        </Body>

        <Joint circular="false" name="FrontalRightKnee" type="hinge">
        <Body>FrontalRightHip</Body>
        <Body>FrontalRightKnee</Body>
        <offsetfrom>FrontalRightKnee</offsetfrom>
        <weight>1.0</weight>
        <limitsdeg>-61.3 82.4</limitsdeg>
        <axis>0 1 0</axis>
        <maxveldeg>30</maxveldeg>
        <resolution>1</resolution>
        </Joint>

        <Body name="r5dummy" type="dynamic">
        <offsetfrom>FrontalRightKnee</offsetfrom>
        <Translation>0 0.0175 -0.3</Translation>
        </Body>

        <Joint name="q5dummy" type="hinge" enable="False">
        <Body>FrontalRightKnee</Body>
        <Body>r5dummy</Body>
        <limitsdeg>0 0</limitsdeg>
        </Joint>

        <Body name="FrontalRightAnkle" type="dynamic">
        <offsetfrom>r5dummy</offsetfrom>
        <Translation>0 0 0</Translation>
        <Geom type="trimesh">
        <Render>models/4.4^pierna_der_links.wrl</Render>
        <collision>models/4.4^pierna_der_links.wrl</collision>
        </Geom>
        <Mass type="custom">
          <density>1</density>
        </Mass>
        </Body>

        <Joint circular="false" name="FrontalRightAnkle" type="hinge">
        <Body>r5dummy</Body>
        <Body>FrontalRightAnkle</Body>
        <offsetfrom>FrontalRightAnkle</offsetfrom>
        <weight>1.0</weight>
        <limitsdeg>-23.1 25.4</limitsdeg>
        <axis>0 1 0</axis>
        <maxveldeg>30</maxveldeg>
        <resolution>1</resolution>
        </Joint>

        <Body name="SaggitalRightAnkle" type="dynamic">
        <offsetfrom>FrontalRightAnkle</offsetfrom>
        <Geom type="trimesh">
        <Render>models/4.5^pierna_der_links.wrl</Render>
        <collision>models/4.5^pierna_der_links.wrl</collision>
        </Geom>
        <Mass type="custom">
          <density>10</density>
        </Mass>
        </Body>

       <Joint circular="false" name="SaggitalRightAnkle" type="hinge">
        <Body>FrontalRightAnkle</Body>
        <Body>SaggitalRightAnkle</Body>
        <offsetfrom>SaggitalRightAnkle</offsetfrom>
        <weight>1.0</weight>
        <limitsdeg>-19.9 42.5</limitsdeg>
        <axis>1 0 0</axis>
        <maxveldeg>30</maxveldeg>
        <resolution>1</resolution>
        </Joint>

        <Body name="RightLegTCP" type="dynamic">
        <offsetfrom>SaggitalRightAnkle</offsetfrom>
        <Translation>0 0 -0.123</Translation>
        </Body>

        <Joint name="q5Man" type="hinge" enable="False">
        <Body>SaggitalRightAnkle</Body>
        <Body>RightLegTCP</Body>
        <limitsdeg>0 0</limitsdeg>
        </Joint>



</KinBody>
