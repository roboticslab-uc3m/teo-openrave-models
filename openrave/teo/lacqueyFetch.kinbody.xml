<KinBody name="lacqueyFetch">

	<!-- Base: FrontalWrist (muÃ±eca + palma) -->

	    <Body name="FrontalWrist" type="dynamic">
             <Translation>0 0 0</Translation>
             <Geom type="trimesh">
              <Render>models/lacqueyFetch/hand_frontal_wrist.wrl 0.001</Render> 
              <collision>models/lacqueyFetch/hand_frontal_wrist.wrl 0.001</collision>
             </Geom>
             <Mass type="custom">
              <density>1</density>
             </Mass>
            </Body>

	<!-- Dedo pulgar (thumb): falange proximal (00) -->

	    <Body name="ThumbProximal" type="dynamic">
            <offsetfrom>FrontalWrist</offsetfrom>
            <Translation>0.027472302 0 -0.164588951</Translation>            
            <Geom type="trimesh">
            <Render>models/lacqueyFetch/hand_phalange_proximal.wrl 0.001</Render>
            <collision>models/lacqueyFetch/hand_phalange_proximal.wrl 0.001</collision>
            </Geom>
            <Mass type="custom">
              <density>1</density>
            </Mass>
            </Body>

	    <Joint circular="false" name="ThumbProximalJoint" type="hinge">
            <Body>FrontalWrist</Body>
            <Body>ThumbProximal</Body>
            <offsetfrom>ThumbProximal</offsetfrom>
	
            <weight>1.0</weight>
            <limitsdeg>-30 10</limitsdeg>
            <axis>0 -1 0</axis>
            <maxveldeg>30</maxveldeg>
            <resolution>1</resolution>

            </Joint>

	<!-- Dedo pulgar (thumb): falange distal (00) -->

	    <Body name="ThumbDistal" type="dynamic">
            <offsetfrom>ThumbProximal</offsetfrom>
            <Translation>0.027 0 -0.047</Translation>	                
            <Geom type="trimesh">
            <Render>models/lacqueyFetch/hand_phalange_distal.wrl 0.001</Render>
            <collision>models/lacqueyFetch/hand_phalange_distal.wrl 0.001</collision>
            </Geom>
            <Mass type="custom">
              <density>1</density>
            </Mass>
            </Body>

	    <Joint circular="false" name="ThumbDistalJoint" type="hinge">
            <Body>ThumbProximal</Body>
            <Body>ThumbDistal</Body>
            <offsetfrom>ThumbDistal</offsetfrom>
            <weight>1.0</weight>
            <limitsdeg>-50 10</limitsdeg>
            <axis>0 -1 0</axis> 	<!-- sentido de giro de la joint -->
            <maxveldeg>30</maxveldeg>
            <resolution>1</resolution>
            </Joint>

	<!-- Dedo indice: falange proximal (01) -->

	    <Body name="ExternalFingerProximal" type="dynamic">
            <offsetfrom>FrontalWrist</offsetfrom>
            <Translation>-0.02852741 -0.02010339 -0.164409358</Translation>
            <RotationAxis>0 0 1 180</RotationAxis>            
            <Geom type="trimesh">
            <Render>models/lacqueyFetch/hand_phalange_proximal.wrl 0.001</Render>
            <collision>models/lacqueyFetch/hand_phalange_proximal.wrl 0.001</collision>
            </Geom>
            <Mass type="custom">
              <density>1</density>
            </Mass>
            </Body>

	    <Joint circular="false" name="ExternalFingerProximalJoint" type="hinge">
            <Body>FrontalWrist</Body>
            <Body>ExternalFingerProximal</Body>
            <offsetfrom>ExternalFingerProximal</offsetfrom>
            <weight>1.0</weight>
            <limitsdeg>-30 10</limitsdeg>
            <axis>0 -1 0</axis>
            <maxveldeg>30</maxveldeg>
            <resolution>1</resolution>
            </Joint>

	<!-- Dedo indice: falange distal (01) -->

	    <Body name="ExternalFingerDistal" type="dynamic">
            <offsetfrom>ExternalFingerProximal</offsetfrom>
            <Translation>0.027 0.0 -0.047</Translation>
            <Geom type="trimesh">
            <Render>models/lacqueyFetch/hand_phalange_distal.wrl 0.001</Render>
            <collision>models/lacqueyFetch/hand_phalange_distal.wrl 0.001</collision>
            </Geom>
            <Mass type="custom">
              <density>1</density>
            </Mass>
            </Body>

	    <Joint circular="false" name="ExternalFingerDistalJoint" type="hinge">
            <Body>ExternalFingerProximal</Body>
            <Body>ExternalFingerDistal</Body>
            <offsetfrom>ExternalFingerDistal</offsetfrom>
            <weight>1.0</weight>
            <limitsdeg>-50 10</limitsdeg>
            <axis>0 -1 0</axis>
            <maxveldeg>30</maxveldeg>
            <resolution>1</resolution>
            </Joint>

	<!-- Dedo medio: falange proximal (02) -->

	    <Body name="InternalFingerProximal" type="dynamic">
            <offsetfrom>FrontalWrist</offsetfrom>
            <Translation>-0.02852741 0.02010339 -0.164409358</Translation>
            <RotationAxis>0 0 1 180</RotationAxis>            
            <Geom type="trimesh">
            <Render>models/lacqueyFetch/hand_phalange_proximal.wrl 0.001</Render>
            <collision>models/lacqueyFetch/hand_phalange_proximal.wrl 0.001</collision>
            </Geom>
            <Mass type="custom">
              <density>1</density>
            </Mass>
            </Body>

	    <Joint circular="false" name="InternalFingerProximalJoint" type="hinge">
            <Body>FrontalWrist</Body>
            <Body>InternalFingerProximal</Body>
            <offsetfrom>InternalFingerProximal</offsetfrom>
            <weight>1.0</weight>
            <limitsdeg>-30 10</limitsdeg>
            <axis>0 -1 0</axis>
            <maxveldeg>30</maxveldeg>
            <resolution>1</resolution>
            </Joint>

	<!-- Dedo medio: falange distal (02) -->

	    <Body name="InternalFingerDistal" type="dynamic">
            <offsetfrom>InternalFingerProximal</offsetfrom>
            <Translation>0.027 0.0 -0.047</Translation>           
            <Geom type="trimesh">
            <Render>models/lacqueyFetch/hand_phalange_distal.wrl 0.001</Render>
            <collision>models/lacqueyFetch/hand_phalange_distal.wrl 0.001</collision>
            </Geom>
            <Mass type="custom">
              <density>1</density>
            </Mass>
            </Body>

	    <Joint circular="false" name="InternalFingerDistalJoint" type="hinge">
            <Body>InternalFingerProximal</Body>
            <Body>InternalFingerDistal</Body>
            <offsetfrom>InternalFingerDistal</offsetfrom>
            <weight>1.0</weight>
            <limitsdeg>-50 10</limitsdeg>
            <axis>0 -1 0</axis>
            <maxveldeg>30</maxveldeg>
            <resolution>1</resolution>
            </Joint>

</KinBody>
